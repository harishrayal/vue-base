{
  "files": [],
    "compilerOptions": {
      "target": "ES5",
      "module": "esnext",
      "strict": true,
      "jsx": "preserve",
      "allowJs": true,
      "importHelpers": true,
      "moduleResolution": "node",
      "experimentalDecorators": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "sourceMap": true,
      "baseUrl": ".",
      "noImplicitAny": false,
      "noImplicitThis": true,
      "noImplicitReturns": true,
      "paths": {
        "@/*": [
         "./*"
        ]
       }
    },
  "references": [
    {
      "path": "./tsconfig.config.json"
    },
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.vitest.json"
    }
  ]
}
